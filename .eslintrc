{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/forbid-prop-types": 0,
    "react/no-multi-comp": 0,
    "quote-props": 0,
    "global-require": 0,
    "no-console": 0,
    "object-curly-spacing": 0,
    "comma-dangle": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/jsx-tag-spacing": 0,
    "react/jsx-space-before-closing": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.js"
      }
    }
  }
}
